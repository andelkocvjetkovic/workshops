{
  "name": "locastic_workshop",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:andelkocvjetkovic/locastic_workshop.git",
  "author": "Andelko Cvjetkovic <and3lko@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite & json-server --watch -p 4000 db.json",
    "vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "json-server": "json-server --watch -p 4000 db.json",
    "lint": "eslint --ext .jsx,.js src",
    "pretty": "prettier --check \"./src/**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix --ext .jsx,.js src",
    "pretty:fix": "prettier --write \"./src/**/*.{js,jsx}\"",
    "fix": "npm run pretty:fix && npm run lint:fix",
    "test": "yarn run jest -env=jsdom"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/material": "5.6.1",
    "@mui/utils": "5.6.1",
    "jest": "27.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.3.0",
    "redux-saga": "1.1.3",
    "axios": "0.26.1",
    "redux": "4.1.2",
    "react-redux": "7.2.8",
    "dayjs": "1.11.1",
    "react-hook-form": "7.30.0",
    "yup": "0.32.11",
    "@hookform/resolvers": "2.8.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.17.0",
    "prop-types": "15.8.1",
    "prettier": "2.6.2",
    "@vitejs/plugin-react": "1.3.1",
    "vite": "2.9.5",
    "json-server": "0.17.0",
    "babel-jest": "27.5.1",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "babel-preset-react-app": "10.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "@babel/core": "7.17.8",
    "@babel/plugin-syntax-jsx": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/mocks/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@app(.*)$": "<rootDir>/src$1"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  }
}
